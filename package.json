  "description": "User Service for managing user profiles in a social media platform",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "nest build",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
